<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN"
    "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module>
    <source path="mk/gdx/firebase"/>
    <super-source path="fakes"/>

    <extend-configuration-property name="gdx.reflect.include" value="mk.gdx.firebase.html"/>

    <extend-configuration-property name="gdx.reflect.exclude" value="mk.gdx.firebase.html.database.DatabaseReference"/>

    <extend-configuration-property name="gdx.reflect.include"
                                   value="mk.gdx.firebase.promises.FuturePromise"/>
    <extend-configuration-property name="gdx.reflect.include"
                                   value="mk.gdx.firebase.callbacks.AuthCallback"/>
    <extend-configuration-property name="gdx.reflect.include"
                                   value="mk.gdx.firebase.callbacks.DataCallback"/>
    <extend-configuration-property name="gdx.reflect.include"
                                   value="mk.gdx.firebase.callbacks.CompleteCallback"/>
    <extend-configuration-property name="gdx.reflect.include"
                                   value="mk.gdx.firebase.callbacks.SignOutCallback"/>
    <extend-configuration-property name="gdx.reflect.include"
                                   value="mk.gdx.firebase.listeners.ConnectedListener"/>
    <extend-configuration-property name="gdx.reflect.include"
                                   value="mk.gdx.firebase.listeners.DataChangeListener"/>
    <extend-configuration-property name="gdx.reflect.include"
                                   value="mk.gdx.firebase.annotations.NestedGenericType"/>
    <extend-configuration-property name="gdx.reflect.include"
                                   value="mk.gdx.firebase.annotations.MapConversion"/>
    <extend-configuration-property name="gdx.reflect.include"
                                   value="mk.gdx.firebase.functional.Consumer"/>
</module>